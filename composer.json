{
    "name": "linkorb/userbase-role-provider-bundle",
    "description": "Userbase Role Provider Bundle for modern Symfony Applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "linkorb/userbase-role-contracts": "^2.0",
        "symfony/config": "^4 || ^5",
        "symfony/dependency-injection": "^4 || ^5",
        "symfony/http-kernel": "^4 || ^5",
        "symfony/polyfill-php73": "^1.11",
        "symfony/yaml": "^4 || ^5"
    },
    "conflict": {
        "userbase/client": "<1.7"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": { "LinkORB\\Userbase\\RoleProviderBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
