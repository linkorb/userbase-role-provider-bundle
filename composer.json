{
    "name": "linkorb/userbase-role-provider-bundle",
    "description": "Userbase Role Provider Bundle for modern Symfony Applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.0.3",
        "linkorb/userbase-role-contracts": "^2.0",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.0"
    },
    "autoload": {
        "psr-4": { "LinkORB\\Userbase\\RoleProviderBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
